{
  "name": "chatbox-widget",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "node node_modules/vite/bin/vite.js",
    "build": "node node_modules/vite/bin/vite.js build",
    "preview": "node node_modules/vite/bin/vite.js preview --host --port 4173",
    "postinstall": "node -e \"try{const fs=require('fs');const p='node_modules/.bin/vite';if(fs.existsSync(p)){fs.chmodSync(p,0o755)}}catch(e){console.warn(e.message)}\""
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.1",
    "vite": "^5.3.1"
  }
}
